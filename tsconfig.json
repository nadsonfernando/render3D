{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": ".",
    "typeRoots": [
      "./src/types"
    ],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
    }
  }
}